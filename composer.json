{
    "name": "seait/student-registration-system",
    "description": "Student Registration System for SEAIT",
    "type": "project",
    "require": {
        "php": ">=7.4,<8.2",
        "phpoffice/phpspreadsheet": "^1.29",
        "ext-mysqli": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "SEAIT\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SEAIT\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "post-install-cmd": [
            "mkdir -p uploads/excel",
            "chmod 755 uploads/excel"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
} 